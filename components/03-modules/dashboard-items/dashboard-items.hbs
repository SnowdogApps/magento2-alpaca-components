<div class="dashboard-items {{ class }}">
    <h3 class="heading heading--fifth-level {{ titleClass }}">
        {{ title }}
    </h3>

    <div class="grid dashboard-items__content {{ contentClass }}">
        {{#each informations }}
            <div class="dashboard-items__item {{ this.class }}">
                <h5 class="heading heading--sixth-level">
                    {{ this.title }}
                </h5>

                <div class="dashboard-items__address">
                    {{ information }}
                </div>

                {{> '@dashboard-actions' actions }}
            </div>
        {{/each}}
    </div>
</div>