<div class="swatch {{ class }}" {{{ attributes }}}>
    <h5 class="swatch__title {{ titleClass }}">
        {{ title }}
    </h5>

    <div class="swatch__handler">
        {{#each options }}
            <div class="swatch__option {{ this.class }}" {{{ this.attributes }}}>
                {{ this.text }}
            </div>
        {{/each }}
    </div>
</div>

<script src="{{ static 'swatch.js' }}"></script>
