<div class="page-wrapper">
    {{ render '@header' }}
    <div class="dashboard container">
        <div class="row">
            <div class="col-xs-12 col-md-4 col-lg-3">
                {{ render '@dashboard-nav' }}
            </div>
            <div class="col-xs-12 col-md-8 col-lg-9 roles-and-permissions">
                {{ render '@title-wrapper' titleWrapper }}
                {{> '@dashboard-table' itemsOrdered }}
                {{ render '@toolbar' toolbar merge=true }}
                {{ render '@dashboard-actions-toolbar' actions }}
            </div>
        </div>
    </div>
</div>
{{ render '@footer' }}
