<div class="page-wrapper">
    {{ render '@header' }}
    <div class="dashboard container">
        <div class="row">
            <div class="col-xs-12 col-md-4 col-lg-3">
                {{ render '@dashboard-nav' }}
            </div>
            <div class="col-xs-12 col-md-8 col-lg-9">
                <div class="requisition-list-view">
                    {{ render '@title-wrapper' titleWrapper }}
                    <p class="requisition-list-view__description">
                        {{ description }}
                    </p>
                </div>
                {{ render '@dashboard-actions-toolbar' }}
            </div>
        </div>
    </div>
</div>
{{ render '@footer' }}
