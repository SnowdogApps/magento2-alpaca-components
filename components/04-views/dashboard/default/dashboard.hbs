{{ render '@header' }}

<div class="dashboard container">
    <div class="grid">
        <div class="grid__column grid__column--1-4 dashboard__sidebar">
            {{ render '@dashboard-nav' }}

            {{> '@dashboard-list-item' wishlist }}

            {{> '@dashboard-list-item' recentlyOrdered }}
        </div>

        <div class="grid__column grid__column--3-4 dashboard__content">
            {{ render '@dashboard-table' }}

            {{> '@dashboard-items' account }}

            {{> '@dashboard-items' addreses }}
        </div>
    </div>
</div>

{{ render '@footer' }}
