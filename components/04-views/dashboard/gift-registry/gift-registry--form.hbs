{{ render '@header' }}

<div class="dashboard container">
    <div class="grid">
        <div class="grid__column grid__column--1-4 dashboard__sidebar">
            {{ render '@dashboard-nav' }}
        </div>

        <div class="grid__column grid__column--3-4 dashboard__content">
            {{> '@dashboard-form' generalInformation }}

            {{> '@dashboard-form' eventInformation }}

            {{> '@dashboard-form' eventInformationSecondPart }}

            {{> '@dashboard-form' registrantInformation }}

            {{> '@action' addRegistrant }}

            {{> '@dashboard-form' shippingAddress }}

            {{> '@dashboard-form' hiddenNewAddressForm }}

            {{> '@action' buttons }}
        </div>
    </div>
</div>

{{ render '@footer' }}

<script src="{{ static '../gift-registry/gift-registry--form.js' }}"></script>
